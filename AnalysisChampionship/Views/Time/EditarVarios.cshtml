@model AnalysisChampionship.Models.Time

@using (Html.BeginForm("SalvarVarios", "Time", FormMethod.Post))
{
    <div class="form-group">
        <textarea class="form-control form-control-user" name="times" autofocus></textarea>
    </div>
    <div class="form-group">
        <input type="hidden" name="Pais" value="@Model.Pais" />
        <label>@Model.Pais</label>
    </div>
    <input type="submit" class="btn btn-primary btn-user btn-block" value="Salvar" />
}


